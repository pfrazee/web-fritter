{
  "name": "socket-signal",
  "version": "9.2.1",
  "description": "Signal abstraction to create WebRTC connections through sockets.",
  "main": "index.js",
  "files": [
    "lib",
    "index.js"
  ],
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "jest --verbose --passWithNoTests --forceExit --detectOpenHandles",
    "posttest": "npm run lint",
    "lint": "xd-ns \"**/*.js\"",
    "version": "chan release --allow-yanked ${npm_package_version} && git add .",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "end-of-stream": "^1.4.4",
    "fastq": "^1.8.0",
    "nanocustomassert": "^1.0.0",
    "nanoerror": "^1.0.0",
    "nanomessage-rpc": "^3.2.0",
    "nanoresource-promise": "^2.0.0",
    "p-event": "^4.1.0",
    "p-limit": "^3.0.2",
    "simple-peer": "^9.6.2"
  },
  "devDependencies": {
    "@geut/chan": "^2.0.0",
    "@geut/xd": "^1.5.0",
    "duplexify": "^4.1.1",
    "jest": "^25.1.0",
    "standard": "^14.3.1",
    "through2": "^3.0.1",
    "wrtc": "^0.4.4"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest",
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/socket-signal.git"
  },
  "keywords": [
    "socket-signal",
    "webrtc",
    "signal",
    "signal-client",
    "signal-server",
    "simple-peer"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/socket-signal/issues"
  },
  "homepage": "https://github.com/geut/socket-signal#readme"
}
